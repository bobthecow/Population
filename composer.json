{
    "name": "bobthecow/population",
    "type": "library",
    "description": "A simpler way to populate your Doctrine 2 databases",
    "license": "MIT",
    "require": {
        "php": ">=5.3.0"
    },
    "suggest": {
        "doctrine/data-fixtures": "Population provides a base data fixtures class",
        "doctrine/mongodb-odm": "Population can populate ODM documents",
        "doctrine/orm": "Population can populate ORM entities"
    },
    "autoload": {
        "psr-0": {
            "Population": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
